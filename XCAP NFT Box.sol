        uint256 xid;
        uint256 ratio;
    }

    mapping(uint256 => BoxItem) private xidRatioList;

    mapping(address => uint256) public accountJoinCount;

    mapping(address => uint256) public tokenConsumePrice;

    mapping(address => bool) public approveToken;

    mapping(address => uint256) public accountPreBox;

