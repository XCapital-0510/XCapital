        external
        view
        returns (address, uint256)
    {
        return (tokenHoldNeedAddr, tokenHoldNeed);
    }

    function setApproveOpenNum(uint256 _num)
        external
    {
        require(tx.origin == owner());
        approveOpenNum = _num;
        if(approveOpenNum > 0) {
            isOpen = true;
        }
    }

    function setXIdRatioList(
        uint256 _index,
        uint256 _xid,
        uint256 _ratio
    )
        external
    {
        require(tx.origin == owner());
        require(_index > 0);
        if(_index > 1) {
            require(_ratio > xidRatioList[_index-1].ratio);
        }

        xidRatioList[_index].xid = _xid;
        xidRatioList[_index].ratio = _ratio;
        if(_index > range) {
            range = _index;
        }
    }

    function getXNftTokenId(address account)
        internal
    {   
        uint256 number = XCommon.random(1, xidRatioList[range].ratio+1, address(account).balance);
        
        accountJoinCount[account] +=1;
        approveOpenNum -= 1;
        if(approveOpenNum == 0) {
            isOpen = false;
        }

        for(uint256 i=1; i < range+1; i++) {
            if(number <= xidRatioList[i].ratio) {
                accountPreBox[account] = XNFT(nftAddr).mint(account, xidRatioList[i].xid);
                break;
            }
        }
    }

    function joinWithToken(address _tokenAddr)
        external
    {
        require(isOpen == true);
        require(approveToken[_tokenAddr] && _tokenAddr != address(0));
        //require(IERC721(nftAddr).balanceOf(address(this)) != 0);
        require(IBEP20(tokenHoldNeedAddr).balanceOf(msg.sender) >= tokenHoldNeed);

        getXNftTokenId(msg.sender);

        IBEP20(_tokenAddr).transferFrom(msg.sender, XOfficeAccount, tokenConsumePrice[_tokenAddr]);
    }

    function joinWithBNB() 
        external
        payable
    {
        require(isOpen == true);
        //address(0) represents BNB
        require(approveToken[address(0)]);
        require(IBEP20(tokenHoldNeedAddr).balanceOf(msg.sender) >= tokenHoldNeed);

        uint256 amount = msg.value;

        getXNftTokenId(msg.sender);

        (bool sent,) = XOfficeAccount.call{value : tokenConsumePrice[address(0)]}("");
        require(sent);
        if(amount > tokenConsumePrice[address(0)]) {
            (sent,) = msg.sender.call{value : amount.sub(tokenConsumePrice[address(0)])}("");
            require(sent);
        }
    }

    function getXIdRatioList()
        external
        view
        returns (uint256[] memory xidList, uint256[] memory ratioList)
    {
        xidList = new uint256[](range);
        ratioList = new uint256[](range);
        for(uint256 i; i < range; i++) {
            xidList[i] = xidRatioList[i+1].xid;
            ratioList[i] = xidRatioList[i+1].ratio;
        }
        return (xidList, ratioList);
    }

    function getKeyAddress()
        public
        view
        returns (address _officeAddr, address _nftAddr)
    {
        _officeAddr = XOfficeAccount;
        _nftAddr = nftAddr;
    }
}

